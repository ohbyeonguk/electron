{
  "name": "web_health_checks",
  "version": "0.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development electron .",
    "build:osx": "cross-env NODE_ENV=production electron-builder --mac",
    "build:linux": "cross-env NODE_ENV=production npm run build:linux32 && npm run build:linux64",
    "build:linux32": "cross-env NODE_ENV=production electron-builder --linux --ia32",
    "build:linux64": "cross-env NODE_ENV=production electron-builder --linux --x64",
    "build:win": "cross-env NODE_ENV=production npm run build:win32 && npm run build:win64",
    "build:win32": "cross-env NODE_ENV=production electron-builder --win portable --ia32",
    "build:win64": "cross-env NODE_ENV=production electron-builder --win portable --x64",
    "deploy:osx": "cross-env NODE_ENV=production electron-builder --mac",
    "deploy:linux": "cross-env NODE_ENV=production npm run build:linux32 && npm run build:linux64",
    "deploy:linux32": "cross-env NODE_ENV=production electron-builder --linux --ia32",
    "deploy:linux64": "cross-env NODE_ENV=production electron-builder --linux --x64",
    "deploy:win": "cross-env NODE_ENV=production npm run build:win32 && npm run build:win64",
    "deploy:win32": "cross-env NODE_ENV=production electron-builder --win portable --ia32",
    "deploy:win64": "cross-env NODE_ENV=production electron-builder --win portable --x64"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0"
  },
  "repository": {
    "type" : "git",
    "url": "https://github.com/ohbyeonguk/electron.git"
  }
}
